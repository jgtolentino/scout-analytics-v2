{
  "name": "scout-analytics-v2",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/api/analytics/route.ts": {
      "maxDuration": 30
    },
    "app/api/retailbot/chat/route.ts": {
      "maxDuration": 30
    }
  }
}